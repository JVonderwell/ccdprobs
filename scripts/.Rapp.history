n <- 100 #star+normal houses#
nsize <- 100 #sample size per m and gap#
#
m <- seq(2,500,by=10)#
gap <- seq(0,0.5,by=0.01)#
nreps=length(m)*length(gap)#
#
pstar=rep(0,nreps)#
p1=rep(0,nreps)#
error=rep(0,nreps)
j = 1#
for(mi in m){#
    for(g in gap){#
        pst <- runif(1,g,1)#
        p1 <- pst-g#
        p2 <- runif(n-1,0,p1)#
        p <- c(pst,p1,p2)#
        p <- p/sum(p)#
#
        pstar[j] <- p[1]#
        p1[j] <- p[2]#
#
        error = 0#
        for(i in 1:nsize){#
            x <- rmultinom(1,mi,p)#
            maxima = which(x==max(x))#
            if(length(maxima) > 1){#
                if(1 %in% maxima){#
                    if(runif(1,0,1) > 0.5){#
                        error = error+1#
                    }#
                } else {#
                    error = error +1#
                }#
            } else {#
                if(which(x==max(x)) != 1){#
                    error = error+1#
                }#
            }#
        }#
        error[j] = error/nsize#
        j = j+1#
    }#
}
