j = 1#
for(mi in m){#
    for(g in gap){#
        #print(mi)#
        #print(g)#
        pst <- runif(1,g,1)#
        p1tmp <- pst-g#
        p2 <- runif(n-1,0,p1tmp)#
        p <- c(pst,p1tmp,p2)#
        p <- p/sum(p)#
        #print(p)#
#
        pstar[j] <- p[1]#
        p1[j] <- p[2]#
        #print(pstar[j])#
