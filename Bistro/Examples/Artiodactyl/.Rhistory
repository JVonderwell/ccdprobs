getS = function(x)
{
    x = as.vector(x)
    r = x[1:6]
    p = x[7:10]
    out = p %o% p
    num = as.vector( out[row(out)>col(out)] )
    s = r*num
    s = s/sum(s)
    return(c(p,s))
}
