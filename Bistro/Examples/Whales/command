## joint vs no joint
time ../../Code/bistro/bistro -f ../../Data/whales.fasta -b 10000 -r 1000 -m 2 -o no-joint -s 709216427 > no-joint.log
time ../../Code/bistro/bistro -f ../../Data/whales.fasta -b 10000 -r 1000 -m 2 -o joint -s 709216427 --jointMLE > joint.log


