## run bistro and bmcmc non fixed true tree
../../Code/bistro/bistro -f ../../Data/sim-artiodactyl-6.fasta -o bistro1 -r 1000 -b 1000 -t "(Camel,Dolphin,(Pig,(Cow,(Sheep,Giraffe))));" > bistro1.log
../../Code/bistro/bmcmc -f ../../Data/sim-artiodactyl-6.fasta -o mcmc1 -r 10000 -b 1000 -t "(Camel,Dolphin,(Pig,(Cow,(Sheep,Giraffe))));" > mcmc1.log
summaryBistroSim("bistro1")
